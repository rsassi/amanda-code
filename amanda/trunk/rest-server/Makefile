install-RestServerpubliccssDATA: $(RestServerpubliccss_DATA)
	@$(NORMAL_INSTALL)
	@list='$(RestServerpubliccss_DATA)'; test -n "$(RestServerpubliccssdir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(RestServerpubliccssdir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(RestServerpubliccssdir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(RestServerpubliccssdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(RestServerpubliccssdir)" || exit $$?; \
	done

