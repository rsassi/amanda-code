install-RestAmandapublicjavascriptsDATA: $(RestAmandapublicjavascripts_DATA)
	@$(NORMAL_INSTALL)
	@list='$(RestAmandapublicjavascripts_DATA)'; test -n "$(RestAmandapublicjavascriptsdir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(RestAmandapublicjavascriptsdir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(RestAmandapublicjavascriptsdir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(RestAmandapublicjavascriptsdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(RestAmandapublicjavascriptsdir)" || exit $$?; \
	done

uninstall-RestAmandapublicjavascriptsDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(RestAmandapublicjavascripts_DATA)'; test -n "$(RestAmandapublicjavascriptsdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(RestAmandapublicjavascriptsdir)'; $(am__uninstall_files_from_dir)
